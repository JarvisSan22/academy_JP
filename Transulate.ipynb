    "# Joining pieces to form the full text\n",
    "plain_text = ' '.join(pieces).replace('\\n', ' ').strip()\n",
    "# Further clean up to remove any residual HTML entities if they exist\n",
    "clean_text = re.sub(r'&[^;\\s]+;', '', plain_text)\n",
    "clean_text"
